									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="BooksDetail">
								<xs:simpleType>
									<xs:restriction base="nonEmptyString">
										<xs:minLength value="1" />
										<xs:maxLength value="100" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element ref="AddressDetail" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
