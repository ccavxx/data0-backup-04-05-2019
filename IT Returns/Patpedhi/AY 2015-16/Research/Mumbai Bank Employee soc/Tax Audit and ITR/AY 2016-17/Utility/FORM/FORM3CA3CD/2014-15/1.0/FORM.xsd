	<xs:complexType name="Declaration">
		<xs:sequence>
			<xs:element name="Point1" type="f3ca:point1" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Point3" type="f3ca:point3" minOccurs="1" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="point1">
		<xs:sequence>
			<xs:element name="I_We1">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="I|We" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="f3ca:AssesseeName"  />
			<xs:element ref="f3ca:AddressDetail"/>
			<xs:element name="PAN">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="[A-Za-z]{5}[0-9]{4}[A-Za-z]" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Me_Us_Ms">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="Me|Us|(M/s)" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CA_FirmName">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:minLength value="1" />
						<xs:maxLength value="75" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Act">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="[a-zA-Z\s0-9'.,#-/ &amp;]{3,100}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="I_We2">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="I|We" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="My_Our_Their">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="My|Our|Their" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AuditDated" type="xs:date" />
			<xs:element name="PointA" type="f3ca:pointA" />
			<xs:element name="AuditYearDate" type="xs:date"/>
			<xs:element name="PointC">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern
							value="Profit and loss account|Income and expenditure account" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="pointA">
		<xs:sequence>
			<xs:element name="TypeOfAccount">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern
							value="Profit and loss account|Income and expenditure account" />
						
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <xs:element name="Year" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-9]{4}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element> -->
			<xs:element name="AuditStartDate" type="xs:date" />
			<xs:element name="AuditEndDate" type="xs:date" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="point3">
		<xs:sequence>
			<xs:element name="My_Our1">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="My|Our" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="My_Our2">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="My|Our" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Me_Us">
				<xs:simpleType>
					<xs:restriction base="Form:nonEmptyString">
						<xs:pattern value="Me|Us" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Observations" minOccurs="0" maxOccurs="unbounded">
			<xs:complexType>
					<xs:sequence>
						<xs:element name="ObservationsCode">
							<xs:simpleType>
								<xs:restriction base="f3cd:nonEmptyString">
									<xs:maxLength value="20" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ObservationsVal">
							<xs:simpleType>
								<xs:restriction base="Form:nonEmptyString">
									<xs:minLength value="1" />
									<xs:maxLength value="1000" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>				
			</xs:element>		
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AddressDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AddrDetail1">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:minLength value="1" />
							<xs:maxLength value="100" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AddrDetail2" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:minLength value="0" />
							<xs:maxLength value="100" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CityOrTownOrDistrict" id="AdrSec5a">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:minLength value="1" />
							<xs:maxLength value="50" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="StateCode" id="AdrSec6a">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:maxLength value="2" />
							<xs:minLength value="1" />
							<xs:pattern value="0[1-9]{1}|1[0-9]{1}|2[0-9]{1}|3[0-6]{1}|99" />							
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PinCode" id="AdrSec7a">
					<xs:simpleType>
						<xs:restriction base="xs:unsignedLong">
							<xs:totalDigits value="6" />
							<xs:pattern value="[1-9]{1}[0-9]{5}" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AssesseeName">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FirstName" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:maxLength value="25" />
							<xs:minLength value="0" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MiddleName" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:maxLength value="25" />
							<xs:minLength value="0" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="LastName" >
					<xs:simpleType>
						<xs:restriction base="Form:nonEmptyString">
							<xs:maxLength value="75" />
							<xs:minLength value="1" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>		
</xs:schema>
